{
  "name": "copy-http",
  "displayName": "Copy Http",
  "version": "0.0.1",
  "description": "__MSG_extensionDescription__",
  "author": "HL",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.22.1",
    "@ant-design/icons": "^5.5.2",
    "antd": "^5.22.5",
    "chalk": "4.1.2",
    "i18next": "^24.2.0",
    "jsonpath-plus": "^10.2.0",
    "plasmo": "0.89.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^15.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chalk": "2.2.4",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*",
      "http://*/*"
    ],
    "permissions": [
      "tabs",
      "storage",
      "webRequest",
      "clipboardWrite",
      "offscreen"
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "*"
        ],
        "matches": [
          "<all_urls>"
        ]
      }
    ],
    "default_locale": "zh_CN"
  }
}
